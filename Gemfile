source "https://rubygems.org"

gem "jekyll", "4.0.0"

# Official Plugins
group :jekyll_plugins do
  gem "jekyll-paginate"
  gem "jekyll-redirect-from"
  gem "jekyll-seo-tag"
end

group :test do
  gem "html-proofer"
end

# Windows does not include zoneinfo files, so bundle the tzinfo-data gem


platform :mswin, :mingw, :x64_mingw, :jruby do
  gem "tzinfo-data"
end

